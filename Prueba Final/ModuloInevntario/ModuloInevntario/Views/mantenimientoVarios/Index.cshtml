@model ModuloInevntario.ViewModels.IndexViewModel

@{
    ViewBag.Title = "Index";
}

<br />
<h3>Mantenimiento de equipos varios</h3>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-striped table-bordered">
    <thead class="thead-dark">
        <tr>
            <th>
                TIPO
            </th>
            <th>
                RESPONSABLE
            </th>
            <th>
                CODIGOINTERNO
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.manteniminetoVario)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.TIPO)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.RESPONSABLE)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CODIGOINTERNO)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.SECUENCIAL }) |
                    @Html.ActionLink("Details", "Details", new { id = item.SECUENCIAL }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.SECUENCIAL })
                </td>
            </tr>
        }
    </tbody>
</table>

@{Html.RenderPartial("_paginador", Model);}
