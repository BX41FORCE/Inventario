@model IEnumerable<ModuloInevntario.Models.ingresoComputadores>

@{
    ViewBag.Title = "Index";
}
<br />
<h3>Ingreso de Computadores</h3>
<script src="~/Scripts/jquery-3.3.1.js"></script>
<p>
    @Html.ActionLink("Ingresar", "Create")
</p>
<table class="table table-responsive table-striped" border="1">
    <thead class="thead-dark">
        <tr>
            <th id="Codigo">
                @Html.DisplayNameFor(model => model.CODIGOINTERNO)
            </th>
            <th id="Ciudad">
                @Html.DisplayNameFor(model => model.CUADADDEUBICACION)
            </th>
            <th id="Descripcion">
                @Html.DisplayNameFor(model => model.DESCRIPCION)
            </th>
            <th id="Ram">
                @Html.DisplayNameFor(model => model.MEMORIARAM)
            </th>
            <th id="Procesador">
                @Html.DisplayNameFor(model => model.PROCESADOR)
            </th>
            <th id="Disco">
                @Html.DisplayNameFor(model => model.DISCODURO)
            </th>
            <th id="Licencia">
                @Html.DisplayNameFor(model => model.LICENCIADO)
            </th>
            <th id="Office">
                @Html.DisplayNameFor(model => model.OFFICE)
            </th>
            <th id="Marca">
                @Html.DisplayNameFor(model => model.MARCA)
            </th>
            <th id="Modelo">
                @Html.DisplayNameFor(model => model.MODELO)
            </th>
            <th id="Serie">
                @Html.DisplayNameFor(model => model.SERIE)
            </th>
            <th id="Particularidad">
                @Html.DisplayNameFor(model => model.PARTICULARIDAD)
            </th>
            <th id="Estado">
                @Html.DisplayNameFor(model => model.ESTADO)
            </th>
            <th id="FacturaN">
                @Html.DisplayNameFor(model => model.NODEFACTURA)
            </th>
            <th id="FacturaV">
                @Html.DisplayNameFor(model => model.VALORFACTURA)
            </th>
            <th id="Fecha">
                @Html.DisplayNameFor(model => model.FECHAADQUISICION)
            </th>
            <th id="Observaciones">
                @Html.DisplayNameFor(model => model.OBSERVACIONES)
            </th>
            <th>.............Opciones...........</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.CODIGOINTERNO)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CUADADDEUBICACION)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DESCRIPCION)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MEMORIARAM)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PROCESADOR)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DISCODURO)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LICENCIADO)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OFFICE)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MARCA)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MODELO)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SERIE)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PARTICULARIDAD)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ESTADO)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NODEFACTURA)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.VALORFACTURA)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FECHAADQUISICION)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OBSERVACIONES)
                </td>
                <td>
                    @Html.ActionLink("Editar", "Edit", new { id = item.SECUENCIAL }) |
                    @Html.ActionLink("Detalle", "Details", new { id = item.SECUENCIAL }) |
                    @Html.ActionLink("Eliminar", "Delete", new { id = item.SECUENCIAL })
                </td>
            </tr>
        }
        </tbody>

</table>
<script>
    $('#Codigo').text('C\u00F3digo');
    $('#Ciudad').text('Ciudad');
    $('#Descripcion').text('Descripci\u00F3n');
    $('#Ram').text('RAM');
    $('#Procesador').text('Procesador');
    $('#Disco').text('Disco');
    $('#Licencia').text('Licenciado');
    $('#Office').text('Office');
    $('#Marca').text('Marca');
    $('#Modelo').text('Modelo');
    $('#Serie').text('Serie');
    $('#Particularidad').text('Particularidad');
    $('#Estado').text('Estado');
    $('#FacturaN').text('NºFactura');
    $('#FacturaV').text('$Factura');
    $('#Fecha').text('Adquirido');
    $('#Observaciones').text('Observaciones');
</script>