@model IEnumerable<ModuloInventario.Models.INGRESOCOMPUTADORES>

@{
    ViewData["Title"] = "Index";
}
<link rel="stylesheet" href="~/css/fontawesome-free-5.3.1-web/css/all.min.css">
<h2>Ingreso de Computadores</h2>
<p>
    <a asp-action="Create">Nuevo</a>
</p>
<table class="table table-responsive" border="1">
    <thead style="background-color:#3072AC; color:#ffffff;">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CODIGOINTERNO)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CUADADDEUBICACION)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ITEM)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DESCRIPCION)
            </th>
            <th>
