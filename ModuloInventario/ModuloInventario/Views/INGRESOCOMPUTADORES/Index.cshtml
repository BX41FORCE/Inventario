            </th>
            <th>
                @Html.DisplayNameFor(model => model.PROCESADOR)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DISCODURO)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SISTEMAOPERATIVO)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LICENCIADO)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OFFICE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MARCA)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MODELO)
