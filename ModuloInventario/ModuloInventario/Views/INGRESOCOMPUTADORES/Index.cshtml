            </th>
            <th>
                @Html.DisplayNameFor(model => model.VALORFACTURA)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FECHAADQUISICION)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OBSERVACIONES)
            </th>
            <th>..............Opciones............</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CODIGOINTERNO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CUADADDEUBICACION)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ITEM)
