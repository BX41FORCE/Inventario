@model IEnumerable<ModuloInventario.Models.ASIGNACIONCOMPUTADORES>

@{
    ViewData["Title"] = "Index";
}
<link rel="stylesheet" href="~/css/fontawesome-free-5.3.1-web/css/all.min.css">
<h2>Asignacion de Computadores</h2>

<p>
    <a asp-action="Create">Nuevo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RESPONSABLE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SECUANCIALPERSONA)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SECUANCIALINGRESOCOMPUTADORES)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TIEMPOINICIO)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TIEMPOFIN)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RESPONSABLE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SECUANCIALPERSONA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SECUANCIALINGRESOCOMPUTADORES)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TIEMPOINICIO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TIEMPOFIN)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SECUENCIAL">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.SECUENCIAL">Detalle</a> |
                <a asp-action="Delete" asp-route-id="@item.SECUENCIAL">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
